#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/jeremy_tek/Desktop/Sebez-Visual-Arts-eCommerce-Website/server/node_modules/nopt/bin/node_modules:/home/jeremy_tek/Desktop/Sebez-Visual-Arts-eCommerce-Website/server/node_modules/nopt/node_modules:/home/jeremy_tek/Desktop/Sebez-Visual-Arts-eCommerce-Website/server/node_modules:/home/jeremy_tek/Desktop/Sebez-Visual-Arts-eCommerce-Website/node_modules:/home/jeremy_tek/Desktop/node_modules:/home/jeremy_tek/node_modules:/home/node_modules:/node_modules:/home/jeremy_tek/Desktop/Sebez-Visual-Arts-eCommerce-Website/server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/jeremy_tek/Desktop/Sebez-Visual-Arts-eCommerce-Website/server/node_modules/nopt/bin/node_modules:/home/jeremy_tek/Desktop/Sebez-Visual-Arts-eCommerce-Website/server/node_modules/nopt/node_modules:/home/jeremy_tek/Desktop/Sebez-Visual-Arts-eCommerce-Website/server/node_modules:/home/jeremy_tek/Desktop/Sebez-Visual-Arts-eCommerce-Website/node_modules:/home/jeremy_tek/Desktop/node_modules:/home/jeremy_tek/node_modules:/home/node_modules:/node_modules:/home/jeremy_tek/Desktop/Sebez-Visual-Arts-eCommerce-Website/server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nopt/bin/nopt.js" "$@"
else
  exec node  "$basedir/../nopt/bin/nopt.js" "$@"
fi
